{
  "name": "reactigation",
  "description": "React-only Navigation for React Native.",
  "version": "0.0.0-development",
  "repository": "github:tobua/reactigation",
  "license": "MIT",
  "author": "Matthias Giger",
  "main": "./index.tsx",
  "types": "./index.tsx",
  "type": "module",
  "exports": {
    ".": "./index.tsx"
  },
  "scripts": {
    "app": "bun create-app.js",
    "copy": "cpx '*.{ts,tsx}' app/node_modules/reactigation --watch",
    "test": "jest",
    "types": "tsc && tsc --noEmit --project ./test/tsconfig.json",
    "lint": "eslint . --fix --ext .ts,.tsx",
    "format": "prettier \"{,!(app|dist)/**/}*.{ts,tsx}\" --write"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-native": ">= 0.70"
  },
  "devDependencies": {
    "@npmcli/arborist": "^7.5.4",
    "@react-native-community/cli": "^14.0.1",
    "@react-native/babel-preset": "^0.75.2",
    "@react-native/eslint-config": "^0.75.2",
    "@react-native/typescript-config": "^0.75.2",
    "@types/bun": "^1.1.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.3",
    "@types/react": "^18.3.5",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "cpx": "^1.5.0",
    "eslint": "8.57.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "npm-packlist": "^8.0.2",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-native": "^0.75.2",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.5.4"
  },
  "files": [
    "*.ts",
    "*.tsx"
  ],
  "keywords": [
    "react-native",
    "navigation",
    "animation"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@react-native",
    "rules": {
      "semi": 0
    },
    "ignorePatterns": [
      "app"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/app/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native)"
    ],
    "moduleNameMapper": {
      "react-dom": "react-native",
      "reactigation": "<rootDir>"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  },
  "publishConfig": {
    "provenance": true
  }
}
